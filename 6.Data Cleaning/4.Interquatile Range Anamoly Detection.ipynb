   "source": [
    "import pandas as pd\n",
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "x=pd.Series([2.3,2.2,4.5,2.1,2.5,2.3])\n",
    "q1,q3=np.percentile(x,[25,75])\n",
    "iqr=q3-q1\n",
    "outliers=[]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[4.5]\n"
     ]
    }
   ],
   "source": [
    "for item in x:\n",
    "    if item<(q1-1.5*iqr) or item>(q3+1.5*iqr):\n",
    "        outliers.append(item)\n",
    "print(outliers)"
   ]
  }
 ],
